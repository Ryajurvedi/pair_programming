        "/api/v1/rooms": {
            "post": {
                "tags": [
                    "Coding"
                ],
                "summary": "Rest Create Room",
                "description": "POST /rooms: Creates a new room. \nAlso logs incoming request headers.",
                "operationId": "rest_create_room_api_v1_rooms_post",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
