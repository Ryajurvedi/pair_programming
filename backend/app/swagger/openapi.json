        "/api/v1/health": {
            "get": {
                "tags": [
                    "Coding"
                ],
                "summary": "Get Health Check",
                "description": "Performs a health check by testing the database connection.\nReturns 200 OK if the database is accessible.",
                "operationId": "get_health_check_api_v1_health_get",
                "responses": {
